{
  "networkName": "TelemedicineBlockchain",
  "version": "1.0.0",
  "genesisTime": "2025-01-19T00:00:00Z",
  "consensusAlgorithm": "PBFT",
  "blockConfig": {
    "blockSize": 1048576,
    "blockInterval": 5
  },
  "roles": [
    {
      "role": "admin",
      "permissions": ["approveUsers", "viewAnalytics", "manageNetwork"]
    },
    {
      "role": "doctor",
      "permissions": ["viewPatientData", "addRecords", "scheduleAppointments"]
    },
    {
      "role": "patient",
      "permissions": ["viewOwnData", "grantAccess", "revokeAccess"]
    }
  ]
}
